   <children>
      <Button layoutX="299.0" layoutY="283.0" mnemonicParsing="false" onAction="#login" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <PasswordField fx:id="txtPass" layoutX="226.0" layoutY="188.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <TextField fx:id="txtUserName" layoutX="226.0" layoutY="137.0" promptText="Cashier Name">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label fx:id="lblStatus" contentDisplay="CENTER" layoutX="174.0" layoutY="47.0" prefHeight="47.0" prefWidth="357.0" text="Status" textAlignment="CENTER" textFill="#f50707">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
